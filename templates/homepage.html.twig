{# Opnemen van de "Main" file #}
{% extends "main.html.twig" %}

{% block header %}
<img src="assets/img/logo-v2.png"/>
{% endblock %}


{# nu de block content op de main pagina vullen #}
{% block content %}
    {# Dit is commentaar #}

    {# Print #}
    {# <h1>Heading #1</h1>
    <h2>Heading #2</h2>
    <h3>Heading #3 t/m 5</h3> #}

    <h4>Dit is de template {{  title }}</h4>

{# Loop #}
    
    {% for record in data %}
        <div class="col-5 border gerecht_blok">
            <img src="{{ record.gerecht.afbeelding }}" style="width: 100px; height: 100px;">
            {{ record.gerecht.titel }} <br>
            {{ record.gerecht.korte_omschrijving}} <br>
            {{ record.waardering}} <br>
            {{ record.prijs}} <br>
            {{ record.gemiddelde_waardering}}
        </div>
    {% endfor %}
     {# { Dump #}
       {# # { <pre>
        {{ dump(data) }}
       </pre>} #}

{% endblock %}

 